{
  "schemaVersion": 1,
  "id": "newchunks-addon",
  "version": "${version}",
  "name": "New Chunks Addon",
  "description": "An addon which adds New Chunks to Meteor Client.",
  "authors": [
    "JonyDev"
  ],
  "contact": {
    "repo": "https://github.com/jonasbina/newchunks"
  },
  "icon": "assets/template/icon.png",
  "environment": "client",
  "entrypoints": {
    "meteor": [
      "com.newchunks.addon.Addon"
    ]
  },
  "mixins": [
    "addon.mixins.json"
  ],
  "custom": {
    "meteor-client:color": "225,25,25"
  },
  "depends": {
    "java": ">=17",
    "minecraft": ">=${mc_version}",
    "meteor-client": "*"
  }
}
